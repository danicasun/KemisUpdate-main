                                                            <paragraphStyle key="NSParagraphStyle" alignment="natural" lineBreakMode="wordWrapping" baseWritingDirection="leftToRight" minimumLineHeight="18.399999618530273" defaultTabInterval="36" tighteningFactorForTruncation="0.0">
                                                                <tabStops/>
