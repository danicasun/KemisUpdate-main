                                                    <fragment content="Online">
                                                        <attributes>
                                                            <color key="NSColor" red="0.26666666666666666" green="0.36078431372549019" blue="0.23529411764705882" alpha="1" colorSpace="custom" customColorSpace="sRGB"/>
